[{
  "name": "Unordered List Quiz",
  "code": "listlistlistlist",
  "tests": [
    {
      "description": "There is an unordered list on the page",
      "definition": {
        "nodes": "ul",
        "exists": true
      }
    },
    {
      "description": "There are three list elements in the unordered list",
      "definition": {
        "nodes": "ul > li",
        "get": "count",
        "equals": 3
      }
    },
    {
      "description": "There is a list element that displays 'HTML'",
      "definition": {
        "nodes": "li",
        "get": "innerHTML",
        "limit": "some",
        "hasSubstring": "HTML"
      }
    },
    {
      "description": "There is a list element that displays 'CSS'",
      "definition": {
        "nodes": "li",
        "get": "innerHTML",
        "limit": "some",
        "hasSubstring": "CSS"
      }
    },
    {
      "description": "There is a list element that displays JavaScript",
      "definition": {
        "nodes": "li",
        "get": "innerHTML",
        "limit": "some",
        "hasSubstring": "(javascript)|(js)|(JavaScript)"
      }
    }
  ]
}]
